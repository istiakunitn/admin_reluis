<h2>Log in</h2>
<%= semantic_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

    <%= f.input :email %>
    <%= f.input :password %>
    <% if devise_mapping.rememberable? -%>
        <%= f.input :remember_me, as: :boolean %>
    <% end -%>

    <%= f.action :submit, label: 'Log in' %>
<% end %>

<%= render "devise/shared/links" %>

